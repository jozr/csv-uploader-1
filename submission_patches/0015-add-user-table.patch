Subject: [PATCH 15/23] add user table

---
 db/migrate/20141203065510_create_users.rb | 10 ++++++++++
 db/schema.rb                              |  9 ++++++++-
 rake                                      |  0
 3 files changed, 18 insertions(+), 1 deletion(-)
 create mode 100644 db/migrate/20141203065510_create_users.rb
 delete mode 100644 rake

diff --git a/db/migrate/20141203065510_create_users.rb b/db/migrate/20141203065510_create_users.rb
new file mode 100644
index 0000000..71c3783
--- /dev/null
+++ b/db/migrate/20141203065510_create_users.rb
@@ -0,0 +1,10 @@
+class CreateUsers < ActiveRecord::Migration
+  def change
+  	create_table :users do |t|
+      t.string :email
+      t.string :password
+
+      t.timestamps
+    end
+  end
+end
diff --git a/db/schema.rb b/db/schema.rb
index b5f6191..ec96f44 100644
--- a/db/schema.rb
+++ b/db/schema.rb
@@ -11,7 +11,7 @@
 #
 # It's strongly recommended that you check this file into your version control system.
 
-ActiveRecord::Schema.define(version: 20141202210836) do
+ActiveRecord::Schema.define(version: 20141203065510) do
 
   # These are extensions that must be enabled in order to support this database
   enable_extension "plpgsql"
@@ -26,4 +26,11 @@ ActiveRecord::Schema.define(version: 20141202210836) do
     t.datetime "created_at"
   end
 
+  create_table "users", force: true do |t|
+    t.string   "email"
+    t.string   "password"
+    t.datetime "created_at"
+    t.datetime "updated_at"
+  end
+
 end
